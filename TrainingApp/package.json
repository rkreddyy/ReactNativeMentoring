    "@react-native-community/netinfo": "^5.8.0",
    "@react-navigation/drawer": "5.0.5",
    "@react-navigation/native": "5.0.5",
    "@react-navigation/stack": "5.0.5",
    "@reduxjs/toolkit": "^1.3.4",
    "appcenter": "3.0.2",
    "appcenter-analytics": "3.0.2",
    "appcenter-crashes": "3.0.2",
